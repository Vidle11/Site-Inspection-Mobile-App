{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "react"
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
